<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
      package="test.sensors"
      android:versionCode="1"
      android:versionName="1.0">
    <uses-sdk android:minSdkVersion="10" />
    <uses-feature android:required="true" android:name="android.hardware.sensor.accelerometer"/>
    <uses-feature android:required="true" android:name="android.hardware.sensor.compass"/>
    
    
    <application android:icon="@drawable/compass" android:label="@string/app_name">
        <activity android:name=".SensorCheckActivity"
                  android:label="@string/app_name">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="MapDraw"></activity>

    </application>
</manifest>